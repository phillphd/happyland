        "sass-loader": "^7.3.1",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "autoprefixer": "^9.8.6",
